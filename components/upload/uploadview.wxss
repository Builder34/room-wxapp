
.upload-frame{
  display: flex;
  flex-wrap: wrap;
  width: 86%;
  margin: 20rpx auto;
  padding: 10rpx 0rpx;
  border-bottom: 1px solid #f6d048;
}

.upload-add-btn{
  width: 140rpx;
  height: 140rpx;
  border: 1px dashed #dfdfdf;
  margin-left: 20rpx;
  margin-bottom: 20rpx;
  font-size: 18rpx;
  color: gray;
  border-radius: 4rpx;
}
.upload-add-btn text{
  display: inline-block;
  vertical-align: middle;
  width: 100%;
  height: 30rpx;
  margin-top: 24rpx;
  text-align: center;
}
.prew-img-view{
  position: relative;
  width: 140rpx;
  height: 140rpx;
  margin-left: 20rpx;
  margin-bottom: 20rpx;
  background: no-repeat center center;
  background-size: cover;
}
.preview-img{
  width: 100%;
  height: 100%;
}
.prew-img-view icon{
  position: absolute;
  top: -18rpx;
  left: 122rpx;
  z-index: 9;
}
